{
  "name": "aws-cicd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:offline": "docker pull lambci/lambda",
    "setup": "./node_modules/.bin/sls config credentials --provider aws --key KEY --secret SECRET",
    "start": "sls offline start -r us-east-1 --noTimeout --corsDisallowCredentials false",
    "install:dynamodb": "./node_modules/.bin/sls dynamodb install",
    "start:dynamodb": "./node_modules/.bin/sls dynamodb start -p 8000  --migrate true",
    "deploy": ".sls deploy --stage dev -v",
    "test": "npm run jest",
    "test:ci": "npm test && npm run coverage",
    "jest": "jest",
    "jest:watch": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SandeepThakare/aws-cicd-practice.git"
  },
  "author": "Sandeep Thakare",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SandeepThakare/aws-cicd-practice/issues"
  },
  "homepage": "https://github.com/SandeepThakare/aws-cicd-practice#readme",
  "dependencies": {
    "aws-sdk": "^2.213.1",
    "serverless-dynamodb-client": "0.0.2",
    "babel-runtime": "^6.26.0",
    "source-map-support": "^0.4.18"
  },
  "devDependencies": {
    "aws-sdk": "^2.224.1",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^3.5.0",
    "jest": "^22.4.3",
    "mocha": "^3.5.3",
    "serverless": "^1.27.2",
    "serverless-dynamodb-local": "^0.2.28",
    "serverless-offline": "^3.20.3",
    "serverless-webpack": "^5.1.0",
    "supertest": "^3.0.0",
    "webpack": "^4.2.0",
    "webpack-node-externals": "^1.6.0"
  }
